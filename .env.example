VITE_SERVER_URL=http://localhost:3000

# Local simulation logs (LocalManager). Defaults to true when unset.
VITE_GAME_SILENT_ROUTING_LOGS=true

# Game manager log allowlist (comma-separated).
# Example: SettlerWorkManager,BuildingManager
VITE_GAME_LOG_ALLOWLIST=SettlerWorkManager

# MultiplayerService debug logs (forwarding + catalog). Defaults to false when unset.
VITE_GAME_MULTIPLAYER_DEBUG=false
